{
  "name": "RecoveryConnect",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npx react-native run-ios --udid 6E31A26B-34ED-4208-BFC5-B6062D55D30A",
    "start": "react-native start",
    "uninstall": "rm -rf node_modules",
    "pod:install": "cd ios && pod install && cd ..",
    "fresh-install": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && npm cache clean --force && npm install",
    "clean:ios": "rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "postinstall": "cd ios && pod install && cd .. && patch-package && npx react-native-asset",
    "test": "jest",
    "lint": "eslint .",
    "migrate:members:install": "cd ../scripts && npm install",
    "migrate:members:dry": "cd ../scripts && npm run migrate-dry-run",
    "migrate:members": "cd ../scripts && npm run migrate",
    "migrate:members:cleanup": "cd ../scripts && npm run cleanup"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.4.1",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-native-community/datetimepicker": "7.6.2",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/slider": "^4.5.6",
    "@react-native-firebase/analytics": "^18.9.0",
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/auth": "^18.7.3",
    "@react-native-firebase/firestore": "^18.9.0",
    "@react-native-firebase/functions": "^18.7.3",
    "@react-native-firebase/storage": "^18.9.0",
    "@react-native-google-signin/google-signin": "^13.2.0",
    "@react-native-picker/picker": "^2.11.0",
    "@react-native-vector-icons/common": "^11.0.0",
    "@react-native-vector-icons/material-icons": "^0.0.1",
    "@react-navigation/bottom-tabs": "^7.3.3",
    "@react-navigation/native": "^7.0.19",
    "@react-navigation/native-stack": "^7.3.3",
    "@react-navigation/stack": "^7.2.3",
    "@reduxjs/toolkit": "^2.7.0",
    "@types/date-fns": "^2.6.3",
    "@types/lodash": "^4.17.16",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-native": "0.72.9",
    "react-native-document-picker": "^9.3.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-fbsdk-next": "^13.4.1",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-google-places-autocomplete": "^2.5.7",
    "react-native-image-picker": "^8.2.0",
    "react-native-maps": "^1.20.1",
    "react-native-permissions": "^5.3.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "^4.0.0",
    "react-native-vector-icons": "^10.0.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.0",
    "@types/react-native-vector-icons": "^6.4.14",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=20"
  }
}
