{
  "name": "RecoveryConnect",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npx react-native run-ios --udid CABB5239-CBF8-4411-B135-EF6CEC671691",
    "start": "react-native start",
    "uninstall": "rm -rf node_modules",
    "pod:install": "cd ios && pod install && cd ..",
    "fresh-install": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && npm cache clean --force && npm install",
